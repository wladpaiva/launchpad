{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --prefix-colors black,#635bff \"bun:dev:*\"",
    "dev:next": "next dev",
    "dev:stripe": "[[ -n $STRIPE_SECRET_KEY ]] && stripe listen --forward-to localhost:3000/api/webhook/stripe || true",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name \"$(date +%s)\"",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset --force",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@analytics/google-analytics": "^1.0.7",
    "@analytics/original-source-plugin": "^1.0.9",
    "@auth/prisma-adapter": "^1.4.0",
    "@epic-web/remember": "^1.0.2",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.1.0",
    "@prisma/client": "^5.10.1",
    "@repo/design-system": "*",
    "@repo/email": "*",
    "@sentry/nextjs": "^7.102.0",
    "@types/mdx": "^2.0.11",
    "@vercel/speed-insights": "^1.0.10",
    "analytics": "^0.8.11",
    "bowlingx-znv": "^0.4.7",
    "next": "^14.1.0",
    "next-auth": "5.0.0-beta.13",
    "next-themes": "^0.2.1",
    "posthog-js": "^1.108.0",
    "prisma": "^5.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-facebook-pixel": "^1.0.4",
    "react-schemaorg": "^2.0.0",
    "resend": "^3.2.0",
    "schema-dts": "^1.1.2",
    "stripe": "^14.17.0",
    "zod": "3.22.3"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.1.0",
    "@playwright/test": "^1.41.2",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/eslint": "^8.56.2",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@unocss/postcss": "^0.58.5",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "localtunnel": "^2.0.2",
    "msw": "^2.2.1",
    "postcss": "^8.4.35",
    "typescript": "^5.3.3",
    "unocss": "^0.58.5"
  },
  "prisma": {
    "seed": "bun prisma/seed.ts"
  }
}
